<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>icapbio</id>
	<name>icap biometrics Module</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.icapbio</package>
	<author>k-joseph</author>
	<description>
		testing 3 available biometrics options
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/icapbio/update.rdf</updateURL>
	<require_version>1.11.5</require_version>
	<!-- / Module Properties -->

	
	<!-- Module Activator -->
	<activator>org.openmrs.module.icapbio.icapbiometricsActivator</activator>
	
	
	<!-- Adds link to admin page -->
	<extension>
		<point>org.openmrs.admin.list</point>
		<class>org.openmrs.module.icapbio.extension.html.AdminList</class>
	</extension>
	
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
		icapbiometrics.hbm.xml
				
	</mappingFiles>

	<!-- Internationalization -->
	<!-- All message codes should start with icapbio. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

